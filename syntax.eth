(import "./eth-lang" eth)

(defmacro quote (node)
  (if (eth.symbol? node) (list 'quote node)
  (if (eth.keyword? node) (list 'quote node)
  (if (eth.list?) node))))

(not defmacro defn (name params ... body)
  (L (S "def") name (L (S "fn") ~name ~params ~@body)))
  `('def name ('fn ~name ~params ~@body))

